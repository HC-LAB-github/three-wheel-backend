{
   "version": 2,
   "name": "nest-three-wheel-demo",
   "builds": [
      {
       "src": "dist/main.js",
       "use": "@vercel/node"
      }
    ],
   "routes": [
      {
       "src": "/(.*)",
       "dest": "dist/main.ts", 
        "methods": [
          "GET",
          "POST",
          "PUT",
          "DELETE",
          "PATCH"
        ]
      }
    ]
 }
